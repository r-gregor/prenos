# ...

ff_personallaunch() {
	selection="$(for URL in "${!URLS[@]}"; do echo "$URL"; done 2>/dev/null | ${FZFCMD_EN})"

	### ADD 20260216###
	if [ "x${selection}" == "x" ]; then
		echo -e "[INFO] nothing selected\n"
		exit
	fi
	### ADD 20260216###

	nohup ${FFCMD_EN} ${URLS["${selection}"]} >&/dev/null &
}

ff_personallaunch

